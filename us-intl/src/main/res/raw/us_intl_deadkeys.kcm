# altgr: left-pointing double angle quotation mark
key LEFT_BRACKET {
    label:                          '['
    base:                           '['
    shift:                          '{'
    ralt:                           '\u00ab'
}

# altgr: right-pointing double angle quotation mark
key RIGHT_BRACKET {
    label:                          ']'
    base:                           ']'
    shift:                          '}'
    ralt:                           '\u00bb'
}

# altgr: not sign
# altgr+shift: broken bar
key BACKSLASH {
    label:                          '\\'
    base:                           '\\'
    shift:                          '|'
    ralt:                           '\u00ac'
    ralt+shift:                     '\u00a6'
}

