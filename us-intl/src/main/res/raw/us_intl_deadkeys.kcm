type OVERLAY

### ROW 1

# combining grave accent
# shift: combining tilde
key GRAVE {
    label:                          '`'
    base:                           '\u0300'
    shift:                          '\u0303'
}

# altgr: inverted exclamation mark
# altgr+shift: superscript one
key 1 {
    label:                          '1'
    base:                           '1'
    shift:                          '!'
    ralt:                           '\u00a1'
    ralt+shift:                     '\u00b9'
}

# altgr: superscript two
key 2 {
    label:                          '2'
    base:                           '2'
    shift:                          '@'
    ralt:                           '\u00b2'
}

# altgr: superscript three
key 3 {
    label:                          '3'
    base:                           '3'
    shift:                          '#'
    ralt:                           '\u00b3'
}

# altgr: currency sign
# altgr+shift: pound sign
key 4 {
    label:                          '4'
    base:                           '4'
    shift:                          '$'
    ralt:                           '\u00a4'
    ralt+shift:                     '\u00a3'
}

# altgr: euro sign
key 5 {
    label:                          '5'
    base:                           '5'
    shift:                          '%'
    ralt:                           '\u20ac'
}

# shift: combining circumflex accent
# altgr: vulgar fraction one quarter
key 6 {
    label:                          '6'
    base:                           '6'
    shift:                          '\u0302'
    ralt:                           '\u00bc'
}

# altgr: vulgar fraction one half
key 7 {
    label:                          '7'
    base:                           '7'
    shift:                          '&'
    ralt:                           '\u00bd'
}

# altgr: vulgar fraction three quarters
key 8 {
    label:                          '8'
    base:                           '8'
    shift:                          '*'
    ralt:                           '\u00be'
}

key 9 {
    label:                          '9'
    base:                           '9'
    shift:                          '('
}

key 0 {
    label:                          '0'
    base:                           '0'
    shift:                          ')'
}

# altgr: yen sign
key MINUS {
    label:                          '-'
    base:                           '-'
    shift:                          '_'
    ralt:                           '\u00a5'
}

# altgr: multiplication sign
# altgr+shift: division sign
key EQUALS {
    label:                          '='
    base:                           '='
    shift:                          '+'
    ralt:                           '\u00d7'
    ralt+shift:                     '\u00f7'
}

